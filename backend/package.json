    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "iconv-lite": "^0.6.3",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.7.5",
    "systeminformation": "^5.27.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
