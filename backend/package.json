    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
